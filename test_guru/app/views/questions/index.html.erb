<h2>Тест</h2>
<table>
  <tr>
      <th>Title</th>
      <th>Level</th>
      <th>Count Questions</th>
  </tr>
<%= render @test, test: @test %>
</table>
<p><%= link_to 'All tests', tests_path%></p>
<h2>Вопросы теста</h2>
<p><%= link_to 'New question', new_test_question_path(@test) %></p>
<table>
  <tr>
      <th>Id</th>
      <th>Body</th>
      <th colspan="3">Action</th>
  </tr>

  <%= render @questions %>
</table>
